<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <corners android:bottomLeftRadius="@dimen/qb_px_4" android:bottomRightRadius="@dimen/qb_px_4" android:topLeftRadius="@dimen/qb_px_4" android:topRightRadius="@dimen/qb_px_4" />

    <item android:state_pressed="false">
        <shape>
            <!--    边框背景 及宽度-->
            <stroke android:width="@dimen/qb_px_1" android:color="@color/cal_days_title" />
            <!-- 内部填充背景色 -->
            <solid android:color="@color/home_blue" />
            <!--    <solid android:color="#F5F5F5" />-->
        </shape>
    </item>


    <item android:state_pressed="true">
        <shape>
            <!--    边框背景 及宽度-->
            <stroke android:width="@dimen/qb_px_1" android:color="@color/home_blue" />
            <!-- 内部填充背景色 -->
            <solid android:color="@color/white" />
            <!--    <solid android:color="#F5F5F5" />-->
        </shape>
    </item>


</selector>

