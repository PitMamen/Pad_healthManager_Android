<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_46"
        >


        <RelativeLayout
            android:id="@+id/rl_back"
            android:layout_width="@dimen/dp_40"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/im_gobackhome"
                android:layout_width="@dimen/qb_px_13"
                android:layout_height="@dimen/qb_px_13"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/qb_px_10"
                android:src="@mipmap/icon_fanhui"></ImageView>
        </RelativeLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="权益使用申请"
            android:textColor="@color/text_black"
            android:textSize="15sp"></TextView>


    </RelativeLayout>

    <View
        android:id="@+id/ll_view"
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:layout_below="@+id/rl_title"
        android:background="#E6E6E6"></View>



    <RelativeLayout
        android:id="@+id/rl_bg"
        android:background="@drawable/shape_white_bg_"
        android:layout_marginTop="@dimen/qb_px_10"
        android:layout_marginStart="@dimen/qb_px_4"
        android:layout_marginEnd="@dimen/qb_px_4"
        android:layout_below="@+id/ll_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="3dp"
        android:translationZ="3dp">


        <RelativeLayout
            android:id="@+id/ll_mc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/qb_px_10"
                android:layout_marginTop="@dimen/qb_px_5"
                android:src="@mipmap/icon_ship"></ImageView>


            <TextView
                android:id="@+id/visit_type_apply"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/qb_px_7"
                android:layout_marginTop="@dimen/qb_px_4"
                android:layout_toRightOf="@+id/icon"
                android:text="视频问诊"
                android:textColor="#666666"
                android:textSize="14.5sp"></TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/qb_px_4"
                android:layout_toRightOf="@+id/visit_type_apply"
                android:text="申请: "
                android:textColor="#666666"
                android:textSize="14.5sp"></TextView>



            <ImageView
                android:id="@+id/iv_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/qb_px_5"
                android:layout_marginRight="@dimen/qb_px_4"
                android:layout_toLeftOf="@+id/tv_end_consultation"
                android:src="@mipmap/icon_jieshu"></ImageView>


            <TextView
                android:id="@+id/tv_end_consultation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/qb_px_4"
                android:layout_marginEnd="@dimen/qb_px_10"
                android:text="结束问诊"
                android:textColor="@color/red">


            </TextView>


        </RelativeLayout>


        <View
            android:id="@+id/ll_line_"
            android:layout_width="match_parent"
            android:layout_height="1.5dp"
            android:layout_below="@+id/ll_mc"
            android:layout_marginStart="@dimen/qb_px_10"
            android:layout_marginTop="@dimen/qb_px_10"
            android:layout_marginEnd="@dimen/qb_px_10"
            android:background="#E6E6E6"></View>




     <RelativeLayout
         android:id="@+id/visit_doc_name"
         android:layout_below="@+id/ll_line_"
         android:layout_width="match_parent"
         android:layout_height="wrap_content">


         <TextView
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginStart="@dimen/qb_px_10"
             android:layout_marginTop="@dimen/qb_px_10"
             android:text="问诊医生"
             android:textColor="@color/text_gray"
             android:textSize="14.5sp"></TextView>


         <TextView
             android:id="@+id/tv_name_by_doc"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignParentRight="true"
             android:layout_marginStart="@dimen/qb_px_10"
             android:layout_marginTop="@dimen/qb_px_10"
             android:layout_marginEnd="@dimen/qb_px_10"
             android:text="王一一"
             android:textColor="@color/text_gray"
             android:textSize="14.5sp"></TextView>



     </RelativeLayout>






        <RelativeLayout
            android:id="@+id/rl_department"
            android:layout_below="@+id/visit_doc_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:text="科室"
                android:textSize="14.5sp"
                android:textColor="@color/text_gray"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>


            <TextView
                android:id="@+id/tv_department"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginEnd="@dimen/qb_px_10"
                android:text="心血管内科"
                android:textColor="@color/text_gray"
                android:textSize="14.5sp"></TextView>



        </RelativeLayout>






        <RelativeLayout
            android:id="@+id/rl_starttime"
            android:layout_below="@+id/rl_department"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:text="开始时间"
                android:textSize="14.5sp"
                android:textColor="@color/text_gray"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>


            <TextView
                android:id="@+id/tv_starttime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginEnd="@dimen/qb_px_10"
                android:text="2022年2月10日 12:00:00"
                android:textColor="@color/text_gray"
                android:textSize="14.5sp"></TextView>



        </RelativeLayout>



        <RelativeLayout
            android:id="@+id/rl_continuetime"
            android:layout_below="@+id/rl_starttime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:text="持续时间"
                android:textSize="14.5sp"
                android:textColor="@color/text_gray"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>


            <TextView
                android:id="@+id/tv_continue_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginEnd="@dimen/qb_px_10"
                android:text="12分钟"
                android:textColor="@color/text_gray"
                android:textSize="14.5sp"></TextView>



        </RelativeLayout>






        <RelativeLayout
            android:id="@+id/rl_patient_name"
            android:layout_below="@+id/rl_continuetime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:text="就诊人"
                android:textSize="14.5sp"
                android:textColor="@color/text_gray"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </TextView>


            <TextView
                android:id="@+id/tv_patient_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginEnd="@dimen/qb_px_10"
                android:text="张三"
                android:textColor="@color/text_gray"
                android:textSize="14.5sp"></TextView>
        </RelativeLayout>



        <View
            android:id="@+id/ll_view_"
            android:layout_marginStart="@dimen/qb_px_10"
            android:layout_marginEnd="@dimen/qb_px_10"
            android:layout_marginTop="@dimen/qb_px_10"
            android:layout_width="match_parent"
            android:layout_height="1.5dp"
            android:layout_below="@+id/rl_patient_name"
            android:background="#E6E6E6"></View>




        <RelativeLayout
            android:layout_below="@+id/ll_view_"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:id="@+id/tv_clickgotodetail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/qb_px_10"
                android:layout_marginTop="@dimen/qb_px_15"
                android:text="点击查看就诊人资料"
                android:textColor="@color/home_blue"
                android:textSize="14.5sp"></TextView>


            <RelativeLayout
                android:id="@+id/rl_start_consultation"
                android:layout_width="@dimen/qb_px_60"
                android:layout_height="37dp"
                android:layout_alignParentRight="true"
                android:layout_marginStart="@dimen/qb_px_5"
                android:layout_marginTop="@dimen/qb_px_10"
                android:layout_marginEnd="@dimen/qb_px_10"
                android:layout_marginBottom="@dimen/qb_px_10"
                android:background="@drawable/shape_comfirm_sele">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="开始问诊"
                    android:textColor="@color/white"
                    android:textSize="15sp">

                </TextView>

            </RelativeLayout>




        </RelativeLayout>


    </RelativeLayout>












</RelativeLayout>