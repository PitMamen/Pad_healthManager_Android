<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_mr_detail_jcjy_jyjgzb"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginLeft="0dp"
    android:layout_marginRight="0dp"
    android:background="@android:color/transparent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp">

        <!-- 竖向分割线 -->
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/white_EBEBEB"/>
        <TextView
            android:id="@+id/tv_sequence"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingVertical="@dimen/qb_px_3"
            android:layout_weight="3"
            android:paddingLeft="@dimen/qb_px_3"
            tools:text="序号"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_8" />

        <!-- 竖向分割线 -->
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="invisible"
            android:background="@color/white_EBEBEB"/>
        <TextView
            android:id="@+id/tv_jczbdm"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingVertical="@dimen/qb_px_3"
            android:layout_weight="5"
            android:paddingLeft="@dimen/qb_px_3"
            android:text="代码"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_8" />

        <!-- 竖向分割线 -->
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="invisible"
            android:background="@color/white_EBEBEB"/>
        <TextView
            android:id="@+id/tv_jczbmc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingVertical="@dimen/qb_px_3"
            android:layout_weight="5"
            android:paddingLeft="@dimen/qb_px_3"
            android:text="检测指标名称"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_8" />

        <!-- 竖向分割线 -->
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="invisible"
            android:background="@color/white_EBEBEB"/>
        <TextView
            android:id="@+id/tv_jczbjg"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingVertical="@dimen/qb_px_3"
            android:layout_weight="5"
            android:paddingLeft="@dimen/qb_px_3"
            android:text="检测指标结果"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_8" />

        <!-- 竖向分割线 -->
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="invisible"
            android:background="@color/white_EBEBEB"/>
        <TextView
            android:id="@+id/tv_jldw"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:paddingVertical="@dimen/qb_px_3"
            android:paddingLeft="@dimen/qb_px_3"
            android:text="计量单位"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_8" />

        <!-- 竖向分割线 -->
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="invisible"
            android:background="@color/white_EBEBEB"/>
        <TextView
            android:id="@+id/tv_ckz"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingVertical="@dimen/qb_px_3"
            android:layout_weight="5"
            android:paddingLeft="@dimen/qb_px_3"
            android:text="参考值范围"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_8" />

        <!-- 竖向分割线 -->
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:visibility="invisible"
            android:background="@color/white_EBEBEB"/>
        <ImageView
            android:id="@+id/tv_ycts"
            android:layout_width="0dp"
            android:layout_height="@dimen/qb_px_8"
            android:layout_weight="5"
            android:paddingLeft="@dimen/qb_px_3"
            android:textColor="@color/black_333333"
            android:layout_gravity="center"
            android:textSize="@dimen/qb_px_8" />

        <!-- 竖向分割线 -->
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/white_EBEBEB"/>
    </LinearLayout>

    <!-- 横向分割线 -->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/white_EBEBEB"/>
</LinearLayout>
