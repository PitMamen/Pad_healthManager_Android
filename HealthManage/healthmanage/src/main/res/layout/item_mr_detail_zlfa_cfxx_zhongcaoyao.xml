<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_mr_detail_zlfa_cfxx_zhongcaoyao"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="0dp"
    android:layout_marginRight="0dp"
    android:background="@android:color/transparent">

    <!-- 中草药 处方 -->
    <LinearLayout
        android:id="@+id/ll_cfxx_zhongyao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_zhongyao_ywmc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="药物名称"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />

        <TextView
            android:id="@+id/tv_zhongyao_ypyf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="药品用法"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />

        <TextView
            android:id="@+id/tv_zhongyao_ypsl_ypdw"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="发药数量 + 发药数量单位"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />

        <TextView
            android:id="@+id/tv_zhongyao_cfts"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="处方贴数"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />

        <TextView
            android:id="@+id/tv_zhongyao_mcsyjl"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="【每次使用剂量】【每次使用剂量单位】"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />

        <TextView
            android:id="@+id/tv_zhongyao_mcsysl"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="【每次使用数量】【每次使用数量单位】"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />

        <TextView
            android:id="@+id/tv_zhongyao_yypc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="用药频次"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />

        <TextView
            android:id="@+id/tv_zhongyao_zyypjsj"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="中药饮片剂数(剂)"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />

        <TextView
            android:id="@+id/tv_zhongyao_zzzf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            tools:text="治则治法"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/qb_px_14" />
    </LinearLayout>

</LinearLayout>
